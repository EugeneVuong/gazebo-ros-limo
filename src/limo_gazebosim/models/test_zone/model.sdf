<?xml version="1.0" ?>
<sdf version="1.6"> <!-- Updated version to 1.6, commonly used now -->
  <model name="test_zone">
    <static>true</static> <!-- Moved static tag inside model, standard practice -->
    <link name="link">
      <inertial>
        <mass>15.0</mass> <!-- Added .0 for clarity -->
        <inertia>
          <!-- Replaced 0.0 with small positive values -->
          <ixx>1.0</ixx>   <!-- Example positive value -->
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>1.0</iyy>   <!-- Example positive value -->
          <iyz>0.0</iyz>
          <izz>0.1</izz>   <!-- Kept original positive value -->
        </inertia>
      </inertial>
      <collision name="collision">
        <!-- Pose format: x y z roll pitch yaw -->
        <pose>-8.12 8.8 0 1.57 0 0</pose>
        <geometry>
          <mesh>
            <!-- URI format is correct if Gazebo finds model://test_zone -->
            <uri>model://test_zone/meshes/building.dae</uri>
          </mesh>
        </geometry>
      </collision>
      <visual name="visual">
         <!-- Pose format: x y z roll pitch yaw -->
        <pose>-8.12 8.8 0 1.57 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://test_zone/meshes/building.dae</uri>
          </mesh>
        </geometry>
      </visual>
    </link>
  </model>
</sdf>